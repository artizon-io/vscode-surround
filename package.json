      "properties": {
        "vscode-surround.custom-patterns": {
          "type": "object",
          "default": {
            "1": {
              "startPattern": "\\(",
              "endPattern": "\\)"
            },
            "2": {
              "startPattern": "function \\w+\\([^\\n]*\\)(: \\w+)? {",
              "endPattern": "}"
            }
          }
        }
      }
    }
